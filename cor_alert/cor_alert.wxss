/* components_simple/cor_alert/cor_alert.wxss */
.corAlert{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0px;
    left: 0px;
    z-index: 12;
    background: rgba(83, 83, 83, 0);
    display: flex;
    align-items: center;
    justify-content: center;
}
.corAlert_content{
    background: rgba(0, 0, 0, 0.705);
    padding: 40rpx 50rpx;
    font-size: 28rpx;
    border-radius: 10rpx;
    max-width: 244rpx;
    line-height: 40rpx;
    text-align: center;
}
.corAlert_content_icon{
    width: 88rpx;
    height: 88rpx;
    display:block;
    margin: auto;
}