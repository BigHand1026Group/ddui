/* components_simple/cor_dialog/cor_dialog.wxss */
.cor_dialog{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0px;
    left: 0px;
    z-index: 100;
    background: rgba(0, 0, 0, 0.5);
    display: flex;
    align-items: center;
    justify-content: center;
    flex-flow: column;
}
.cor_dialog_content{
    width: 560rpx;
    min-height: 550rpx;
    background: white;
    border-radius: 10rpx;
    color: #333333;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-flow: column;
    position: relative;
    /* top: -5%; */
}
.cor_dialog_content_title{
    /* padding: 20rpx; */
    font-size:38rpx;
    font-weight:bold;
    color:rgba(51,51,51,1);
    width: 100%;
    height: 106rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border-bottom: rgba(238,238,238,1) solid 1px;
}
.cor_dialog_content_texts{
    flex: 1;
    display: flex;
    align-items: center;
}
.cor_dialog_content_texts.slot{    
    align-items: initial;
    width: 100%;
    height: 100%;
}
.cor_dialog_content_btns{
    display: flex;
    width: 100%;
    height: 86rpx;
   
    border-top: rgba(238,238,238,1) solid 1px;
}
.cor_dialog_content_btn{
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
   
}
.cancel{
    flex: 1;
    border-right: rgba(238,238,238,1) solid 1px; 
}
.confirm{
    flex: 1;
    text-align: center;
}
.cor_dialog_content_btn_text{
    font-size:28rpx;
    font-weight:500;
    color:#666666;
}
.cancelText{
    
}
.confirmText{
    color:rgba(20,152,255,1);

}

/* ↓↓↓↓↓↓↓↓↓↓ 关闭按钮 */
.close{
    width: 100%;
    /* background: red; */
    margin-top: 80rpx;
}
.close_img{
    width: 70rpx;
    height: 70rpx;
    display:block;
    margin: auto;
}
