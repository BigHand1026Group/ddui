/* components_simple/cor_nextBar/cor_nextBar.wxss */
.cor_nextBar{
    height: 110rpx;
    background: white;
    padding: 0px 30rpx;
    box-sizing: border-box;
}
.cor_nextBar_content_icon{
    display: flex;
    align-items: center;
}
.cor_nextBar_content_icon_image{
    width: 14rpx;
    height: 26rpx;
    margin-left:22rpx;
}
.cor_nextBar_content{
    display: flex;
    height: 100%;
    align-items: center;
    justify-content: space-between;
    border-bottom:#EEEEEE solid 1px;
    box-sizing: border-box;
}
.cor_nextBar_content_texts{
    display: flex;
    flex: 1;
}
.cor_nextBar_content_texts_tip{
    font-size:34rpx;
    font-weight:500;
    color:rgba(51,51,51,1);
    min-width: 200rpx;
}
.cor_nextBar_content_texts_content{
    font-size:34rpx;
    line-height: 36rpx;
    font-weight:500;
    color:rgba(102,102,102,1);
    flex: 1;
    text-align: right;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    white-space: normal;
}
.cor_nextBar_content_texts_content.left{
    text-align: left;
}